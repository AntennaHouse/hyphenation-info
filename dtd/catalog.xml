<!--
  An XML Catalog that rewrites any reference to 'hyphenation.dtd' to
  'hyphenation-info.dtd' in the local directory.
-->
<catalog  xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
<systemSuffix
  id = "hyphenation-dtd"
  systemIdSuffix = "hyphenation.dtd"
  uri = "hyphenation-info.dtd" />
</catalog>
